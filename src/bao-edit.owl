Prefix(owl:=<http://www.w3.org/2002/07/owl#>)
Prefix(rdf:=<http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
Prefix(xml:=<http://www.w3.org/XML/1998/namespace>)
Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(rdfs:=<http://www.w3.org/2000/01/rdf-schema#>)


Ontology(<http://purl.obolibrary.org/obo/bao-edit.owl>
Annotation(rdfs:comment "An ontology for biogeographic areas such as ecoregions, ecozones, and bioregions. This ontology will attempt to complement existing gazetteers and ontologies such as the Environment Ontology.")

Declaration(Class(<http://purl.obolibrary.org/obo/BAO_00000000>))
Declaration(Class(<http://purl.obolibrary.org/obo/BAO_00000001>))
Declaration(Class(<http://purl.obolibrary.org/obo/BAO_00000002>))
Declaration(Class(<http://purl.obolibrary.org/obo/BAO_00000003>))
Declaration(Class(<http://purl.obolibrary.org/obo/BAO_00000004>))
Declaration(Class(<http://purl.obolibrary.org/obo/BAO_00000005>))
Declaration(Class(<http://purl.obolibrary.org/obo/BAO_00000006>))
Declaration(Class(<http://purl.obolibrary.org/obo/BAO_00000007>))
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/BAO_00000000> "biogeographic area"@en)
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/BAO_00000001> "operationally-defined biogeographic area"@en)
SubClassOf(<http://purl.obolibrary.org/obo/BAO_00000001> <http://purl.obolibrary.org/obo/BAO_00000000>)
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/BAO_00000002> "phylogeny"@en)
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/BAO_00000003> "\"areageny\" site history"@en)
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/BAO_00000004> "area of endemism"@en)
SubClassOf(<http://purl.obolibrary.org/obo/BAO_00000004> <http://purl.obolibrary.org/obo/BAO_00000000>)
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/BAO_00000005> "endemic area"@en)
SubClassOf(<http://purl.obolibrary.org/obo/BAO_00000005> <http://purl.obolibrary.org/obo/BAO_00000000>)
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/BAO_00000006> "geographic area"@en)
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/BAO_00000007> "environmental system"@en)
)